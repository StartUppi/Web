{

  "manifest_version": 2,
  "name": "New Tab Redirect",
  "version": "2.0.2",
  "description": " This add-on automatically opens your website every time you open a new tab in your browser.",

"icons": {
  "512": "icon512x512.png"
},

"permissions":
[
"storage"
],
 




"browser_specific_settings": {
  "gecko": {
    "id": "{ca76a51e-39c5-426e-a65a-17331611382e}",
    "strict_min_version": "58.0"  }
},

"options_ui": {
  "page": "options.html"
},



"browser_action": {
    "browser_style": true,
    "default_popup": "options.html",
    "default_icon": {
      "512": "icon512x512.png"
    }
  },


"chrome_url_overrides" : {
    "newtab": "new-tab-redirect.html"
  }
  
}
