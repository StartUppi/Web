{
  "manifest_version": 2,
  "name": "Localhost Search HTTPS",
  "version": "1.2.2",
  "description": "url https://localhost/?q= as Search engine",
  
"icons": {
  "512": "icon512x512.png"
},

  "chrome_settings_overrides": {
  "search_provider": {
    "name": "Localhost (https)",
    "search_url": "https://localhost/?q={searchTerms}",
    "keyword": "@localhost",
    "favicon_url": "https://localhost/favicon.ico"
  }
}

}
